{
    "family": "extraction-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "data-extractor",
        "image": "slimd12/data_extractor:latest",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8000,
            "protocol": "tcp"
          }
        ]
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
  }
  